@model Slk.Web.Models.GuideViewModel

@*<h2>title</h2>

<div class="page-header">
    <div class="text-primary"></div>
</div>*@

<div class="row">
    <div class="col-md-3">

        <div class="list-group">
            @foreach (var cat in Model.Categories)
            {
                @*<a href="#" class="list-group-item">@cat.Title</a>*@
                @Html.ActionLink(cat.Title, "Index", new {id = cat.Id}, new {@class = "list-group-item" + (Model.ActiveCategoryId == cat.Id ? " active" : "")})
                ;
            }

        </div>

        <div id="users">
           
        </div>

    </div>

    <div class="col-md-9">

        @foreach (var art in Model.Articles)
        {
            <div class="media">
                <div class="media-left">
                    <a href="#">
                        <img alt="64x64" class="media-object" data-src="holder.js/64x64" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
                    </a>
                </div>
                <div class="media-body">
                    <h4 class="media-heading">@art.Title</h4>
                    @art.Body
                </div>
            </div>
        }
    </div>
</div>

